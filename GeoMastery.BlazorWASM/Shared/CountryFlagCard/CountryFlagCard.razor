@using System.Text.RegularExpressions;

<div class="card">
    <div class="card-body">
        <h3 class="card-title">@CardViewModel.Country</h3>
        <img src="@($"data:image/png;base64,{CardViewModel.FlagBase64}")" alt="@CardViewModel.Country flag" />
    </div>
</div>

@code {
    [Parameter]
    public CountryFlagCardViewModel CardViewModel { get; set; }
}